<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html lang="pt">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Criar Usuário</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="css/styles.css">
</head>
<body class="d-flex justify-content-center align-items-center vh-100 bg-light">

<div class="container text-center">
    <label class="form-label fw-bold fs-4">NOME</label>
    <input type="text" class="form-control bg-dark text-white mb-3" placeholder="Digite seu nome">

    <label class="form-label fw-bold fs-4">SENHA</label>
    <input type="password" class="form-control bg-dark text-white mb-3" placeholder="Digite sua senha">

    <button class="btn btn-warning rounded-pill btn-lg mb-2">CRIAR USUÁRIO</button><br>
    <button class="btn btn-danger rounded-pill btn-lg">VOLTAR</button>
</div>

</body>
</html>

